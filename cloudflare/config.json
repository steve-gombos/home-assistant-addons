{
  "name": "Cloudflare",
  "version": "1.3",
  "slug": "cloudflare",
  "description": "Manage Cloudflare DNS hosting, DDNS, and DNS challenge certificates",
  "url": "https://github.com/steve-gombos/home-assistant-addons",
  "arch": [ "armhf", "armv7", "aarch64", "amd64", "i386" ],
  "startup": "services",
  "boot": "auto",
  "map": [ "ssl:rw" ],
  "options": {
    "lets_encrypt": {
      "accept_terms": false,
      "certfile": "fullchain.pem",
      "keyfile": "privkey.pem"
    },
    "token": null,
    "zone": null,
    "domains": [ null ],
    "proxy": false,
    "seconds": 300
  },
  "schema": {
    "lets_encrypt": {
      "accept_terms": "bool",
      "certfile": "str",
      "keyfile": "str"
    },
    "token": "str",
    "zone": "str",
    "domains": [ "str" ],
    "proxy": "bool?",
    "seconds": "int"
  }
}
